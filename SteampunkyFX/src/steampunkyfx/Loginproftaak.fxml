            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help" fx:id="MIhelp">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <TabPane id="Createuser" layoutX="0.0" layoutY="0.0" prefHeight="276.0" prefWidth="300.0" tabClosingPolicy="UNAVAILABLE">
          <tabs>
            <Tab fx:id="loginuser" text="Login user">
              <content>
                <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>
                    <Button id="Btexit" fx:id="BTExitlogin" layoutX="161.0" layoutY="123.0" mnemonicParsing="false" text="Exit" />
                    <Button id="Btcreate" fx:id="BTloginuserlogin" layoutX="35.0" layoutY="123.0" mnemonicParsing="false" text="Login user" />
                    <Label id="BtUsername" fx:id="LBUsernamelogin" layoutX="43.0" layoutY="45.0" text="Username" />
                    <Label id="Btwachtwoord" fx:id="LBWachtwoordlogin" layoutX="29.0" layoutY="76.0" text="Wachtwoord" />
                    <TextField id="TFUsername" fx:id="TFWachtwoordlogin" layoutX="115.0" layoutY="73.0" prefWidth="171.0" />
                    <TextField id="TFWachtwoord" fx:id="TFUsernamelogin" layoutX="115.0" layoutY="42.0" prefWidth="171.0" />
