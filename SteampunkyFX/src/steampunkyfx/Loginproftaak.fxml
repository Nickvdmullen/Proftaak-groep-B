                    <Button id="Btcreate" fx:id="BtCreatecreate" layoutX="38.0" layoutY="145.0" mnemonicParsing="false" text="Create user" />
                    <Label id="BtUsername" fx:id="LBUsernamecreate" layoutX="39.0" layoutY="39.0" text="Username" />
                    <Label id="Btwachtwoord" fx:id="LBWachtwoordcreate" layoutX="39.0" layoutY="76.0" text="Wachtwoord" />
                    <TextField id="TFUsername" fx:id="TFUsernamecreate" layoutX="116.0" layoutY="39.0" prefWidth="171.0" />
                    <TextField id="TFWachtwoord" fx:id="TFWachtwoordcreate" layoutX="115.0" layoutY="73.0" prefWidth="171.0" />
                  </children>
                </AnchorPane>
              </content>
            </Tab>
          </tabs>
        </TabPane>
      </children>
    </AnchorPane>
  </children>
</VBox>
